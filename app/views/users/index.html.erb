<%= render 'users/list', user: @user, book: @book %>
<div class="col-xs-9">
 <h2>Users</h2>
 <div class="container">
 	<table class="table">
		<thead>
	 	<tr>
 			<th>Image</th>
 			<th>Name</th>
 		</tr>
 		</thead>
 		<% @users.each do |user| %>
 		<tbody>
 			<td><span class="image_tag"><%= attachment_image_tag user, :image, :fill, 50,50, fallback: "No_img.jpg"%></span></td>
	 		<td><span><%= user.name %></span></td>
 			<td><span> <%= link_to "Show", user_path(user.id) %></span></td>
	 	</tbody>
	 	<% end %>
 	</table>
 </div>
</div>

<!-- 部分テンプレート用のcontainer締めのdiv -->
</div>
</div>